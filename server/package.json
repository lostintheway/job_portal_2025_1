{
  "name": "server",
  "version": "1.0.0",
  "private": true,
  "main": "src/server.ts",
  "type": "module",
  "scripts": {
    "start": "node --experimental-strip-types --watch src/server.ts",
    "dev": "node --experimental-strip-types --watch src/server.ts",
    "tsc": "tsc --init",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.39.0",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "1.4.5-lts.1",
    "mysql2": "^3.12.0",
    "swagger-autogen": "^2.23.7",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.7",
    "drizzle-kit": "^0.30.3",
    "nodemon": "^3.1.9",
    "tsx": "^4.19.2",
    "vitest": "^1.4.0",
    "@vitest/coverage-v8": "^1.4.0"
  }
}
